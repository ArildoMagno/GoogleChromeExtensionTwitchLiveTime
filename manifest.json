{
    "name": "Twitch get timestamp",
    "author": "Arildo Magno",
    "manifest_version": 2,
    "version": "1.0",
    "description": "Get the timestamp from current live",
    "content_scripts": [
        {
            "matches": [
                "http://*/*"
            ],
            "js": [
                "popup.js"
            ]
        }
    ],
    "icons": {
        "1": "icontwitchgettime.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Get Twitch TimeStamp"
    },
    "permissions": [
        "tabs",
        "notifications",
        "http://*/",
        "https://*/",
        "storage",
        "downloads"
    ]
}